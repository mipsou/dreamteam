[project]
name = "dreamteam"
version = "0.1.0"
description = "DreamTeam manage all crewai team"
authors = [{ name = "Mipsou", email = "you@example.com" }]
requires-python = ">=3.10,<3.13"
dependencies = [
    "crewai[tools]>=0.117.1,<1.0.0"
]

[project.scripts]
dreamteam = "dreamteam.cli:cli"

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"

[tool.crewai]
type = "crew"
